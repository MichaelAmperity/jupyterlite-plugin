{
  "@jupyterlab/apputils-extension:themes": {
    "theme": "amperitycolors"
  },

  "@jlab-enhanced/cell-toolbar:plugin":{
    "helperButtons": null,
    "leftMenu": null,
    "floatPosition": {
        "right": 20,
        "top": 10
    },
  "rightMenu": [
    {
      "command": "notebook:run-cell",
      "icon": "ui-components:run",
      "cellType": "code"
    },
    {
      "command": "sqltag",
      "icon": "ui-components:list",
      "cellType": "code"
    },
    {
      "command": "notebook:delete-cell",
      "icon": "@jlab-enhanced/cell-toolbar:delete"
    }]
  },
  "@jupyterlab/cell-toolbar-extension:plugin":{
  "toolbar": [
      {
          "name": "duplicate-cell",
          "args": {},
          "command": "notebook:duplicate-below",
          "disabled": true,
          "rank": 50
      },
      {
          "name": "move-cell-up",
          "args": {},
          "command": "notebook:move-cell-up",
          "disabled": true,
          "rank": 50
      },
      {
          "name": "move-cell-down",
          "args": {},
          "command": "notebook:move-cell-down",
          "disabled": true,
          "rank": 50
      },
      {
          "name": "insert-cell-above",
          "args": {},
          "command": "notebook:insert-cell-above",
          "disabled": true,
          "rank": 50
      },
      {
          "name": "insert-cell-below",
          "args": {},
          "command": "notebook:insert-cell-below",
          "disabled": true,
          "rank": 50
      },
      {
          "name": "delete-cell",
          "args": {},
          "command": "notebook:delete-cell",
          "disabled": true,
          "icon": "ui-components:delete",
          "rank": 50
      }
  ]
}
}